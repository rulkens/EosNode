{
  "paths" : {
    "src"  : {
      "app"         : "app/",
      "js"          : "app/",
      "components"  : "app/",
      "sass"        : "app/_style/",
      "views"       : "app/_views/",
      "images"      : "app/_images/",
      "api"         : "app/_settings/api/",
      "content"     : {
        "main"      : "content/",
        "common"    : "content/common/"
      },
      "templates"   : "templates/"
    },
    "dest" : {
      "main"        : "public/",
      "index"       : "public/",

      "css"         : "public/style/",
      "js"          : "public/script/",
      "views"       : "public/templates/",
      "images"      : "public/images/",
      "fonts"       : "public/style/fonts/",

      "api"         : "public/api/",
      "content"     : "public/content/"
    },
    "public" : {
      "css"         : "style/",
      "js"          : "script/",
      "views"       : "templates/",
      "images"      : "images/",
      "fonts"       : "style/fonts/",

      "api"         : "api/"
    },
    "tmp" : {
      "main"        : ".tmp/",
      "sass"        : ".tmp/sass/",
      "components"  : ".tmp/sass/components/"
    },

    "settings"      : "_settings/"
  },
  "files" : {
    "src"  : {
      "html" : {
        "main"      : "index.html"
      },
      "sass" : {
        "main"      : "main.scss"
      },
      "js": {
        "main"      : "entry.js"
      }
    },
    "dest" : {
      "js" : {
        "app"       : "app",
        "vendor"    : "vendor",
        "head"      : "head"
      },
      "html" : {
        "main"      : "index.html"
      },
      "css" : {
        "main"      : "main.css"
      }
    },
    "vendor" : "vendor.json"
  },

  "versions" : {
    "default"       : "main"
  },
  "env" : {
    "default"       : "dev"
  },
  "globalDefs"      : "",
  "isDevelopment": true,
  "dependencies": ["redux", "react", "react-dom", "classnames/bind"]
}